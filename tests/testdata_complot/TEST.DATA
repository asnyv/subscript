--==============================================================================
--		Synthetic reservoir simulation model Box (2021)
--==============================================================================

--------------------------------------------------------------------------------
--RUNSPEC SECTION
--------------------------------------------------------------------------------

RUNSPEC

-- Simulation run title
TITLE
Simple 2D box model

NOECHO

-- Simulation grid dimension (Imax, Jmax, Kmax)
DIMENS
    31  1   31  /

-- Simulation run start
START
 1 JAN 2000 /

--Activates data check only option
-- NOSIM

-- Fluid phases present
OIL
GAS
DISGAS
WATER

-- Measurement unit used
METRIC

GRIDOPTS
   'YES'      1*       /

--Table dimensions
TABDIMS
-- NTSFUN NTPVT NSSFUN NPPVT NTFIP NRPVT ROCKNUM
     1      1     200    24    6    20 6*      1  /

--Regions dimension data
REGDIMS
-- NTFIP NMFIPR NRFREG NTFREG
    1    1      0      0      /

--Dimension for well data
WELLDIMS
 100  100 100 100 /

WSEGDIMS
10 100 50 50 /

--Summary file dimensions
SMRYDIMS
 200000  /

-- Input and output files format
UNIFIN
UNIFOUT

-- Disables the initial index file output
NOINSPEC

-- Disables the restart index file output
NORSSPEC

ACTDIMS
 200 200 120 /

UDQDIMS
 50 5* 50 /

-------------------------------------------------------------------------
--GRID SECTION
-------------------------------------------------------------------------

GRID

--Disable echoing of the input file  
NOECHO

--Requests output of an INIT file
INIT

--Control output of the Grid geometry file
GRIDFILE
  1 1  /

--Message print and stop limits
MESSAGES
 1* 1* 1* 1000 1* 1* 1* 1* 1000000 10000 0 /

NOECHO

--Include simulation grid
INCLUDE
  '2d_tilt.grdecl' /

EQUALS
  PORO   0.2  1 31  1 1  1 31 /
  PERMX  100  1 31  1 1  1 31 /
  NTG    1.0  1 31  1 1  1 31 /
/

COPY
  PERMX PERMY /
  PERMX PERMZ /
/

--Set Kv to Kh ratio 
MULTIPLY
 PERMZ   1.0e-8   /
/

-------------------------------------------------------------------------
--EDIT SECTION
-------------------------------------------------------------------------

EDIT

EQUALS
 MULTPV 1000.0  1  1  1 1  1 19 / -- Gas reservoir
 MULTPV 1000.0 31 31  1 1  7 31 / -- Aquifer
/

